<BorderPane prefHeight="700.0" prefWidth="900.0" stylesheets="@home.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.cv_builder.HomeController">
   <bottom>
    <Label fx:id="note" contentDisplay="CENTER" prefHeight="30.0" prefWidth="952.0" text="Select the above 'Create CV' button." textAlignment="CENTER" textFill="#dd0b0b" BorderPane.alignment="CENTER">
         <BorderPane.margin>
            <Insets bottom="26.0" />
         </BorderPane.margin></Label>
   </bottom>
   <center>
      <Button fx:id="createButton" mnemonicParsing="false" onAction="#openCreateCV" text="Create CV" BorderPane.alignment="CENTER">
       </Button>
   </center>
   <top>
      <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="welcomeText" prefHeight="27.0" prefWidth="930.0" text="Welcome">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="to" prefHeight="30.0" prefWidth="916.0" text="to">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
            <Label fx:id="welcomeText2" prefHeight="40.0" prefWidth="899.0" text="&quot;CV BuildeR&quot;">
               <VBox.margin>
                  <Insets top="5.0" />
               </VBox.margin>
            </Label>
         </children>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </top>
</BorderPane>
